{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "crons": [
    {
      "path": "/src/controllers/notifications/sendExpoNotification",
      "schedule": "* * * * *"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
